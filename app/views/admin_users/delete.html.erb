<h1>AdminUsers#delete</h1>
<p>Find me in app/views/admin_users/delete.html.erb</p>

<% @page_title = "Delete Admin User" %>

<%= link_to("<< Back to List", admin_users_path, :class => "back-link")%>

<div class="admin-users delete">
    <h2>Delete Admin user</h2>

    <%= form_for(@admin_user, :method => "delete") do |f|%>
        <p>Are you sure you want to permanently delete this admin user ? </p>

        <p class="reference-name"><%= f.object.name %></p>
        <div class="form-buttons">
            <%= f.submit(   "Delete  Admin User")%>
        </div>
    <% end %>
</div>